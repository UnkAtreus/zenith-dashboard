{
    "compilerOptions": {
        "baseUrl": "./",
        "jsx": "react",
        "paths": {
           
            
            "@/store/*": [
                "./src/store/*"
            ],
            "@/assets/*": [
                "./src/assets/*"
            ],
            "@/styles/": [
                "./src/assets/styles/*"
            ],
            "@/utilities/*": [
                "./src/utilities/*"
            ],
            "@/components/*": [
                "./src/components/*"
            ],
            "@/pages/*": [
                "./src/pages/*"
            ],
            "@/src/*": [
                "./src/*"
            ]
        },
    },
    "include": [
        "src/**/*"
, "node_modules/src"    ],
    "exclude": [
        "node_modules"
    ]
}